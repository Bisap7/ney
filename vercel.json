cat > vercel.json << 'EOF'
{
  "version": 3,
  "routes": [
    {
      "src": "/videos/small/(.*\.mp4)",
      "headers": {
        "Content-Type": "video/mp4",
        "Cache-Control": "public, max-age=31536000"
      },
      "continue": true
    },
    {"handle": "filesystem"},
    {"src": "/(.*)", "dest": "/index.html"}
  ]
}
EOF
